App:
    @package = "App"
    /address:
        GET(ok <: foo):
            return ok <: sequence of App.foo
    !type foo:
        this <: string
        that <: int
Bar:
    /bar:
        GET:
            App <- GET /address
            return ok <: foo
    /address:
        GET:
            return ok <: sequence of foo
    !type foo:
        this <: string
        that <: Bar.tar
    !type tar:
        this <: string
        that <: int